  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "mocha": "^4.1.0",
    "rimraf": "^2.6.2",
    "bluebird": "^3.5.1"
  }
